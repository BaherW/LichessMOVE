{
  "name": "Lichess Move!",
  "version": "1.0",
  "description": "My friends screaming at you if you take too long to move",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.lichess.org/*"
      ],
      "js": [
        "counter.js"
      ]
    }
  ]
}